{
	"profiles": {
		"http": {
			"commandName": "Project",
			"launchBrowser": true,
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"dotnetRunMessages": true,
			"applicationUrl": "http://localhost:7682"
		},
		"https": {
			"commandName": "Project",
			"launchBrowser": true,
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"dotnetRunMessages": true,
			"applicationUrl": "http://localhost:7682;https://localhost:7683"
		},
		"IIS Express": {
			"commandName": "IISExpress",
			"launchBrowser": true,
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		},
		"Container (Dockerfile)": {
			"commandName": "Docker",
			"launchBrowser": true,
			"launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
			"environmentVariables": {
				"ASPNETCORE_HTTP_PORTS": "7682",
				"ASPNETCORE_HTTPS_PORTS": "7683"
			},
			"publishAllPorts": true,
			"useSSL": true,
			"applicationUrl": "http://localhost:7682"
		}
	},
	"iisSettings": {
		"windowsAuthentication": false,
		"anonymousAuthentication": true,
		"iisExpress": {
			"applicationUrl": "http://localhost:7682",
			"sslPort": 7683
		}
	}
}